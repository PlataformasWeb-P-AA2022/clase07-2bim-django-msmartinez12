  return {\n                // tasks\n                tasks: [''],\n                nombre: '',\n                apellido: '',\n                correo: '',\n                cedula: '',\n            }\n        },\n        methods: {\n            async getData() {\n                try {\n                    // fetch tasks\n                    const response = await this.$http.get('http://localhost:8000/api/numerost/');\n                    // set the data returned as tasks\n                    this.tasks = response.data;\n                } catch (error) {\n                    // log the error\n                    console.log(error);\n                }\n            },\n        },\n        created() {\n            // Fetch tasks on page load\n            this.getData();\n        }\n    }\n</script>"]},"metadata":{},"sourceType":"module"}